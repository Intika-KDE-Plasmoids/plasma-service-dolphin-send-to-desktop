[Desktop Entry]
Type=Service
ServiceTypes=KonqPopupMenu/Plugin
MimeType=all/all;
X-KDE-StartupNotify=false
X-KDE-Priority=TopLevel
Actions=sendDesktop;
InitialPreference=99
Version=5.21

[Desktop Action sendDesktop]
Name=Send to Desktop (ln -s)
Exec=ln -s %F ~/Desktop
Icon=desktop
